class Solution:
    def fourSumCount(self, A: List[int], B: List[int], C: List[int], D: List[int]) -> int:
        record = {}
        for a in A:
            for b in B:
                if a+b in record:
                    record[a+b] += 1
                else:
                    record[a+b] = 1
        count = 0
        for c in C:
            for d in D:
                if -c-d in record:
                    count += record[-c-d]
        return count
