class Solution:
    def wordBreak(self, s: str, wordDict: List[str]) -> bool:
        dp = [False] * len(s)
        for i in range(len(s)):
            for word in wordDict:
                if i-len(word)+1 >= 0 and s[i-len(word)+1:i+1] == word and (dp[i-len(word)] or i-len(word) == -1):
                    dp[i] = True
        return dp[-1]
