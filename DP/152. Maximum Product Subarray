class Solution:
    def maxProduct(self, nums: List[int]) -> int:
        dp = [-float('inf')]*len(nums)
        dp[0] = nums[0]
        dpn = [0]*len(nums)
        for i in range(1,len(nums)):
            dpn[i] = min(dp[i-1]*nums[i], nums[i], dpn[i-1]*nums[i])
            dp[i] = max(dp[i-1]*nums[i], nums[i], dpn[i-1]*nums[i])
        return max(dp)
