class Solution:
    def findTargetSumWays(self, nums: List[int], S: int) -> int:
        if not nums:
            return 0
        record = {nums[0]:1, -nums[0]:1} if nums[0] != 0 else {0:2}
        for i in range(1,len(nums)):
            temp = {}
            for j in record:
                temp[j+nums[i]] = temp.get(j+nums[i], 0) + record.get(j, 0)
                temp[j-nums[i]] = temp.get(j-nums[i], 0) + record.get(j, 0)
            record = temp
        return record.get(S, 0)
