class Solution:
    def canPartition(self, nums: List[int]) -> bool:
        total = sum(nums)
        if total % 2:
            return False
        dp = [True]+[False]*(total//2)
        for i in range(0,len(nums)):
            for j in range(total//2,0,-1):
                dp[j] = dp[j] or (j-nums[i] >= 0 and dp[j-nums[i]])
        return dp[-1]
