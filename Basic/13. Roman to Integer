class Solution:
    def romanToInt(self, s):
        """
        :type s: str
        :rtype: int
        """

        record = {'M': 1000, 'D': 500 , 'C': 100, 'L': 50, 'X': 10,'V': 5,'I': 1}

        pre = total = 0
        
        for i in range(len(s)-1, -1, -1):
            cur = record[s[i]]
            if cur < pre:
                total -= cur
            else:
                total += cur
            pre = cur
        
        return total
