class Solution:
    def groupAnagrams(self, strs):
        """
        :type strs: List[str]
        :rtype: List[List[str]]
        """
        d = {}
        for i in strs:
            temp = ''.join(sorted(i))
            if temp in d:
                d[temp].append(i)
            else:
                d[temp] = [i]
        res = []
        for j in d:
            res.append(d[j])
        return res
