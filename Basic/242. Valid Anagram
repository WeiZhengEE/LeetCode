class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        record = {}
        for i in s:
            if i not in record:
                record[i] = 1
            else:
                record[i] += 1
        for i in t:
            if i not in record:
                return False
            else:
                record[i] -= 1
        
        for i in record:
            if record[i] != 0:
                return False
            
        return True
