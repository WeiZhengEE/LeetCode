class Solution:
    def findUnsortedSubarray(self, nums: List[int]) -> int:
        temp = sorted(nums)
        for i in range(0, len(nums)):
            if nums[i] != temp[i]:
                for j in range(len(nums)-1,-1,-1):
                    if nums[j] != temp[j]:
                        return j-i+1
        return 0
