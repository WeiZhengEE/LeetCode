class Solution(object):
    def myAtoi(self, str):
        ls = list(str.strip())
        
        if len(ls) == 0: 
            return 0
        
        if ls[0] == "-":
            sign = -1
        else:
            sign = 1
        
        if ls[0] in ['-','+'] : 
            ls.pop(0)
            
        ret, i = 0, 0
        while i < len(ls) and ls[i].isdigit() :
            ret = ret*10 + ord(ls[i]) - ord('0')
            i += 1
        return max(-2**31, min(sign * ret,2**31-1))
