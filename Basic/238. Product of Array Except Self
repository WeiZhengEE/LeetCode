class Solution:
    def productExceptSelf(self, nums: List[int]) -> List[int]:
        res = []
        n = len(nums)
        p = 1
        for i in range(n):
            res.append(p)
            p = p*nums[i]
        p = 1
        for i in range(n-1,-1,-1):
            res[i] = res[i]*p
            p = p*nums[i]
        return res
