class Solution:
    def setZeroes(self, matrix):
        """
        :type matrix: List[List[int]]
        :rtype: void Do not return anything, modify matrix in-place instead.
        """
        m =len(matrix)
        n = len(matrix[0])
        iset = []
        jset = []
        for i in range(m):
            for j in range(n):
                if matrix[i][j] == 0:
                    if i not in iset:
                        iset.append(i)
                    if j not in jset:
                        jset.append(j)
        for i in range(m):
            for j in range(n):
                if (i in iset) or (j in jset):
                    matrix[i][j] = 0
