class Solution:
    def findDuplicate(self, nums: List[int]) -> int:
        slow = 0
        fast = 0
        while nums[fast]:
            slow = nums[slow]
            fast = nums[nums[fast]]
            if fast == slow:
                fast = 0
                while nums[fast]:
                    if slow == fast:
                        return fast
                    slow = nums[slow]
                    fast = nums[fast]
        return -1
